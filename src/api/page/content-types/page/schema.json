{
  "kind": "singleType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Site Settings"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "siteName": {
      "type": "string",
      "required": true,
      "default": "E-Sunrise Australia"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "component",
      "repeatable": false,
      "component": "shared.address"
    },
    "socials": {
      "type": "component",
      "repeatable": true,
      "component": "shared.social-link"
    },
    "defaultSeo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "schemaOrg": {
      "type": "component",
      "repeatable": false,
      "component": "shared.org"
    }
  }
} 